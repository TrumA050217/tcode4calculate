<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zzuli.mapper.AnswerMapper">

    <resultMap id="BaseResultMap" type="com.zzuli.entity.Answer">
            <id property="answerId" column="answer_id" jdbcType="BIGINT"/>
            <result property="bankId" column="bank_id" jdbcType="BIGINT"/>
            <result property="recordId" column="record_id" jdbcType="BIGINT"/>
