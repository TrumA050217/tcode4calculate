        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zzuli.mapper.MyResultMapper">

    <resultMap id="BaseResultMap" type="com.zzuli.entity.MyResult">
            <id property="resultId" column="result_id" jdbcType="BIGINT"/>
            <result property="bankId" column="bank_id" jdbcType="BIGINT"/>
